@article{Shibata2015,
author = {Hiroyuki Shibata and Kaoru Shimizu and Hiroki Takesue and Yasuhiro Tokura},
journal = {Opt. Lett.},
keywords = {Photodetectors; Quantum detectors; Quantum detectors; Femtosecond fiber lasers; Femtosecond lasers; Laser communications; Optical standards; Tunable lasers; Wavelength division multiplexers},
