@article{Shibata_2013,
	doi = {10.7567/apex.6.072801},
	url = {https://doi.org/10.7567percent2Fapex.6.072801},
	year = {2013},
	month = {jul},
	publisher = {{IOP} Publishing},
	volume = {6},
	number = {7},
	pages = {072801},
	author = {Hiroyuki Shibata and Kaoru Shimizu and Hiroki Takesue and Yasuhiro Tokura},
	title = {Superconducting Nanowire Single-Photon Detector with Ultralow Dark Count Rate Using Cold Optical Filters},
	journal = {Applied Physics Express},
	abstract = {We report the fabrication of a superconducting nanowire single-photon detector (SSPD or SNSPD) with an ultralow dark count rate. By introducing optical band-pass filters at the input of the SSPD and cooling the filters at 3 K, the dark count rate is reduced to less than 1/100 at low bias. An SSPD with 0.1 cps dark count rate and 5.6 percent  system detection efficiency at 1550 nm wavelength is obtained. We show that a quantum key distribution (QKD) over 300 km of fiber is possible based on a numerical calculation assuming a differential phase shift QKD protocol implemented with our SSPDs.}
}

@article{Li_16,
author = {Hao Li and Sijing Chen and Lixing You and Wengdong Meng and Zhibo Wu and Zhongping Zhang and Kai Tang and Lu Zhang and Weijun Zhang and Xiaoyan Yang and Xiaoyu Liu and Zhen Wang and Xiaoming Xie},
journal = {Opt. Express},
keywords = {Photodetectors; Optical design and fabrication; Optical devices; Fluorescence spectroscopy; Laser communications; Laser ranging; Photonic crystals; Quantum key distribution; Single photon detectors},
number = {4},
pages = {3535--3542},
publisher = {OSA},
title = {Superconducting nanowire single photon detector at 532 nm and demonstration in satellite laser ranging},
volume = {24},
month = {Feb},
year = {2016},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-24-4-3535},
doi = {10.1364/OE.24.003535},
abstract = {Superconducting nanowire single-photon detectors (SNSPDs) at a wavelength of 532 nm were designed and fabricated aiming to satellite laser ranging (SLR) applications. The NbN SNSPDs were fabricated on one-dimensional photonic crystals with a sensitive-area diameter of 42 \&\#x03BC;m. The devices were coupled with multimode fiber (\&\#x03D5; $=$ 50 \&\#x03BC;m) and exhibited a maximum system detection efficiency of 75 percent  at an extremely low dark count rate of \&lt;0.1 Hz. An SLR experiment using an SNSPD at a wavelength of 532 nm was successfully demonstrated. The results showed a depth ranging with a precision of ~8.0 mm for the target satellite LARES, which is ~3,000 km away from the ground ranging station at the Sheshan Observatory.},
}

@article{Wollman:17,
author = {E. E. Wollman and V. B. Verma and A. D. Beyer and R. M. Briggs and B. Korzh and J. P. Allmaras and F. Marsili and A. E. Lita and R. P. Mirin and S. W. Nam and M. D. Shaw},
journal = {Opt. Express},
keywords = {Photodetectors; Quantum detectors; Ultraviolet; Charge coupled devices; Fluorescence lifetime imaging; Laser ranging; Microscopy; Quantum information processing; Ultraviolet astronomy},
number = {22},
pages = {26792--26801},
publisher = {OSA},
title = {UV superconducting nanowire single-photon detectors with high efficiency, low noise, and 4 K operating temperature},
volume = {25},
month = {Oct},
year = {2017},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-25-22-26792},
doi = {10.1364/OE.25.026792},
abstract = {For photon-counting applications at ultraviolet wavelengths, there are currently no detectors that combine high efficiency (\&\#x0003E; 50\&\#x00025;), sub-nanosecond timing resolution, and sub-Hz dark count rates. Superconducting nanowire single-photon detectors (SNSPDs) have seen success over the past decade for photon-counting applications in the near-infrared, but little work has been done to optimize SNSPDs for wavelengths below 400 nm. Here, we describe the design, fabrication, and characterization of UV SNSPDs operating at wavelengths between 250 and 370 nm. The detectors have active areas up to 56 \&\#x003BC;m in diameter, 70 \&\#x02013; 80\&\#x00025; efficiency at temperatures up to 4.2 K, timing resolution down to 60 ps FWHM, blindness to visible and infrared photons, and dark count rates of \&\#x0223C; 0.25 counts/hr for a 56 \&\#x003BC;m diameter pixel. These performance metrics make UV SNSPDs ideal for applications in trapped-ion quantum information processing, lidar studies of the upper atmosphere, UV fluorescent-lifetime imaging microscopy, and photon-starved UV astronomy.},
}

