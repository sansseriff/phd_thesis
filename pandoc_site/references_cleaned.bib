@article{Shibata2014,
author = {Hiroyuki Shibata and Toshimori Honjo and Kaoru Shimizu},
journal = {Opt. Lett.},
keywords = {Quantum detectors; Quantum detectors; Quantum cryptography ; Bandpass filters; Bit error rate; Phase shift; Photon counting; Quantum key distribution; Single photon detectors},
number = {17},
